{
  "name": "sandervspl-stack",
  "version": "0.1.0",
  "private": true,
  "description": "Repository of sandervspl-stack",
  "author": "Sander Vispoel @sandervspl",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "next dev -p $([ -f .port ] && cat .port || echo 3000)",
    "dev": "npm run start",
    "build": "next build",
    "postbuild": "next-sitemap",
    "build:static": "next build && next export",
    "server": "next start",
    "lint": "eslint --ext .ts --ext .tsx --ext .js src/",
    "analyze": "BUNDLE_ANALYZE=both npm run build",
    "typecheck": "tsc --noEmit",
    "test": "npx concurrently \"npm run lint\" \"npm run typecheck\""
  },
  "dependencies": {
    "express": "4.18.1",
    "lodash": "4.17.21",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "^18.2.0",
    "react-query": "3.39.1",
    "sharp": "0.30.6"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-syntax-flow": "^7.17.12",
    "@babel/plugin-transform-react-jsx": "^7.17.12",
    "@next/bundle-analyzer": "12.1.6",
    "@next/env": "^12.1.6",
    "@next/eslint-plugin-next": "12.1.6",
    "@svgr/webpack": "6.2.1",
    "@tailwindcss/typography": "^0.5.2",
    "@types/express": "4.17.13",
    "@types/lodash": "4.14.182",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "autoprefixer": "^10.4.7",
    "daisyui": "^2.15.4",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "next-pwa": "5.5.4",
    "next-sitemap": "^3.1.3",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.3",
    "ts-toolbelt": "9.6.0",
    "typescript": "4.7.4",
    "webpack": "5.73.0"
  }
}
